{
  "name": "yjs-server",
  "version": "0.0.0",
  "description": "Yjs Server",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": true,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src ./src/index.ts",
