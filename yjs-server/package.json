    "format": "prettier --write",
    "clean": "rm -rf node_modules && rm -rf dist"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
   "@types/lodash.debounce": "4.0.7",
    "@types/node": "^17.0.15",
    "@types/ws": "8.5.3",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-turbo": "latest",
    "eslint-plugin-react": "7.31.0",
    "nodemon": "2.0.20",
    "prettier": "latest",
    "ts-node": "10.9.1",
    "typescript": "^4.5.3",
    "yjs": ">=13.5"
  },
  "dependencies": {
    "@aomao/plugin-yjs-websocket": "latest"
  },
  "peerDependencies": {
    "yjs": ">=13.5"
  },
  "license": "MIT"
