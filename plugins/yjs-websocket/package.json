{
	"name": "@aomao/plugin-yjs-websocket",
	"version": "1.0.5",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.esm.js"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./*": {
			"import": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.esm.js"
			},
			"require": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"server": [
				"dist/server.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"lib",
		"src"
	],
	"author": "me@aomao.com",
	"license": "MIT",
	"homepage": "https://github.com/big-camel/am-editor#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/big-camel/am-editor.git"
	},
	"bugs": {
		"url": "https://github.com/big-camel/am-editor/issues"
	},
	"devDependencies": {
		"@types/lodash.debounce": "4.0.7",
		"@types/mongodb": "4.0.7",
		"@types/ws": "8.5.3",
		"yjs": ">=13.5"
	},
	"dependencies": {
		"@aomao/plugin-yjs-protocols": "^1.0.1",
		"@babel/runtime": "^7.13.10",
		"lib0": "^0.2.52",
		"lodash.debounce": "^4.0.8"
	},
	"peerDependencies": {
		"yjs": ">=13.5"
	},
	"optionalDependencies": {
		"mongodb": "5.0.1",
		"ws": "^6.2.1",
		"y-leveldb": "^0.1.2"
	}
}
