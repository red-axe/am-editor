{
	"name": "@aomao/plugin-yjs-protocols",
	"version": "1.0.1",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.esm.js"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./*": {
			"import": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.esm.js"
			},
			"require": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"sync": [
				"dist/sync.d.ts"
			],
			"awareness": [
				"dist/awareness.d.ts"
			],
			"auth": [
				"dist/auth.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"lib",
		"src"
	],
	"author": "me@aomao.com",
	"license": "MIT",
	"homepage": "https://github.com/big-camel/am-editor#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/big-camel/am-editor.git"
	},
	"bugs": {
		"url": "https://github.com/big-camel/am-editor/issues"
	},
	"devDependencies": {
		"yjs": ">=13.5"
	},
	"dependencies": {
		"@babel/runtime": "^7.13.10",
		"lib0": "^0.2.52"
	},
	"peerDependencies": {
		"yjs": ">=13.5"
	}
}
